    <!-- JavaScript -->
    <script>
        // Progress animation
        const progressKeyframes = `
            @keyframes progressAnimation {
                0% { width: 0%; }
                33% { width: 30%; }
                66% { width: 70%; }
                100% { width: 100%; }
            }
        `;
        const style = document.createElement('style');
        style.textContent = progressKeyframes;
        document.head.appendChild(style);

        // Show form when CTA button is clicked
        document.getElementById('startAnalysisBtn').addEventListener('click', function() {
            // Remove hidden class to show the form
            document.getElementById('formSection').classList.remove('hidden');

            // Smooth scroll to the form section  
            document.getElementById('formSection').scrollIntoView({ 
                behavior: 'smooth',
                block: 'start' 
            });

            // Start progress animation if progressBar exists
            const progressBar = document.getElementById('progressBar');
            if (progressBar) {
                progressBar.style.animation = 'progressAnimation 3s ease-in-out forwards';
            }
        });    <script>
        // CTA Button Event Listener
        document.addEventListener('DOMContentLoaded', function() {
            const startBtn = document.getElementById('startAnalysisBtn');
            const formSection = document.getElementById('formSection');

            if (startBtn && formSection) {
                startBtn.addEventListener('click', function() {
                    // Show the form section
                    formSection.classList.remove('hidden');

                    // Smooth scroll to form
                    formSection.scrollIntoView({ 
                        behavior: 'smooth',
                        block: 'start'
                    });

                    // Optional: Hide the button after clicking
                    startBtn.style.display = 'none';
                });
            } else {
                console.error('CTA Button or Form Section not found');
            }
        });
    </script>    <script>
        // COMPREHENSIVE DEBUG SCRIPT FOR CTA BUTTON
        console.log('🟢 DEBUG: JavaScript file loaded at:', new Date().toISOString());

        // Function to show visual debug messages on page
        function showDebugMessage(message, type = 'info') {
            console.log('🔧 DEBUG:', message);

            // Create visual debug div if it doesn't exist
            let debugDiv = document.getElementById('debugMessages');
            if (!debugDiv) {
                debugDiv = document.createElement('div');
                debugDiv.id = 'debugMessages';
                debugDiv.style.cssText = `
                    position: fixed;
                    top: 10px;
                    right: 10px;
                    background: rgba(0, 0, 0, 0.9);
                    color: white;
                    padding: 10px;
                    border-radius: 5px;
                    font-family: monospace;
                    font-size: 12px;
                    max-width: 300px;
                    z-index: 10000;
                    border: 2px solid #00ffff;
                `;
                document.body.appendChild(debugDiv);
            }

            const timestamp = new Date().toLocaleTimeString();
            const color = type === 'error' ? '#ff4444' : type === 'success' ? '#44ff44' : '#44aaff';
            debugDiv.innerHTML += `<div style="color: ${color}; margin: 2px 0;">[${timestamp}] ${message}</div>`;

            // Keep only last 10 messages
            const messages = debugDiv.querySelectorAll('div');
            if (messages.length > 10) {
                messages[0].remove();
            }
        }

        // Test immediate execution
        showDebugMessage('Script execution started', 'info');

        // Check for required elements immediately
        setTimeout(function() {
            showDebugMessage('Checking for DOM elements...', 'info');

            const button = document.getElementById('startAnalysisBtn');
            const form = document.getElementById('formSection');

            if (button) {
                showDebugMessage('✅ Button found: ' + button.tagName, 'success');
            } else {
                showDebugMessage('❌ Button NOT found!', 'error');
            }

            if (form) {
                showDebugMessage('✅ Form found: ' + form.classList.toString(), 'success');
            } else {
                showDebugMessage('❌ Form NOT found!', 'error');
            }
        }, 100);

        // Main event listener with extensive debugging
        document.addEventListener('DOMContentLoaded', function() {
            showDebugMessage('🟢 DOMContentLoaded fired', 'success');

            try {
                const startBtn = document.getElementById('startAnalysisBtn');
                const formSection = document.getElementById('formSection');

                if (!startBtn) {
                    showDebugMessage('❌ ERROR: startAnalysisBtn not found', 'error');
                    alert('DEBUG ERROR: Button with ID "startAnalysisBtn" not found!');
                    return;
                }

                if (!formSection) {
                    showDebugMessage('❌ ERROR: formSection not found', 'error');
                    alert('DEBUG ERROR: Form section with ID "formSection" not found!');
                    return;
                }

                showDebugMessage('✅ Both elements found successfully', 'success');

                // Add click event listener with debugging
                startBtn.addEventListener('click', function(event) {
                    showDebugMessage('🎯 BUTTON CLICKED!', 'success');
                    alert('DEBUG: Button click registered! Form should appear now.');

                    try {
                        // Check if form is currently hidden
                        const isHidden = formSection.classList.contains('hidden');
                        showDebugMessage('Form currently hidden: ' + isHidden, 'info');

                        if (isHidden) {
                            // Remove hidden class
                            formSection.classList.remove('hidden');
                            showDebugMessage('✅ Removed hidden class', 'success');

                            // Verify class was removed
                            const stillHidden = formSection.classList.contains('hidden');
                            showDebugMessage('Still hidden after removal: ' + stillHidden, stillHidden ? 'error' : 'success');

                            // Scroll to form
                            setTimeout(function() {
                                formSection.scrollIntoView({ 
                                    behavior: 'smooth',
                                    block: 'start'
                                });
                                showDebugMessage('✅ Scrolled to form', 'success');
                            }, 100);

                            // Hide button to prevent multiple clicks
                            startBtn.style.display = 'none';
                            showDebugMessage('✅ Button hidden', 'success');

                        } else {
                            showDebugMessage('ℹ️ Form already visible', 'info');
                        }

                    } catch (error) {
                        showDebugMessage('❌ Error in click handler: ' + error.message, 'error');
                        alert('DEBUG ERROR in click handler: ' + error.message);
                    }
                });

                showDebugMessage('✅ Event listener attached successfully', 'success');

                // Additional testing - make button glow to show it's ready
                startBtn.style.boxShadow = '0 0 20px #00ff00';
                showDebugMessage('✅ Button styling updated (green glow)', 'success');

            } catch (error) {
                showDebugMessage('❌ CRITICAL ERROR: ' + error.message, 'error');
                alert('CRITICAL DEBUG ERROR: ' + error.message);
            }
        });

        // Fallback mechanism - if JavaScript fails, provide alternative
        window.addEventListener('load', function() {
            setTimeout(function() {
                showDebugMessage('🔄 Running fallback check...', 'info');

                const button = document.getElementById('startAnalysisBtn');
                if (button && !button.style.boxShadow.includes('00ff00')) {
                    showDebugMessage('⚠️ Event listener may have failed, adding fallback', 'error');

                    // Direct onclick fallback
                    button.onclick = function() {
                        alert('FALLBACK: Using direct onclick handler');
                        const form = document.getElementById('formSection');
                        if (form) {
                            form.classList.remove('hidden');
                            form.scrollIntoView({ behavior: 'smooth' });
                            button.style.display = 'none';
                        }
                    };
                }
            }, 2000);
        });
    </script>
